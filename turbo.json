{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "./packages/**/*.js",
        "./packages/**/*.js.map",
        "./packages/**/*.cjs",
        "./packages/**/*.cjs.map",
        "./packages/**/*.d.ts",
        "./packages/**/*.d.cts",
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
